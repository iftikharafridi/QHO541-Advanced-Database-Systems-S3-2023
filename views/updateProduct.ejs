<!DOCTYPE html>
<html>

<head>
  <%- include('layouts/header'); -%>
  <title>Contact</title>
</head>

<body>
  <div class="hero_area">
    <!-- header section strats -->
    <header class="header_section">
      <%- include('layouts/navbar'); -%>
    </header>
    <!-- end header section -->

  </div>
  <!-- end hero area -->

  <!-- contact section -->

  <section class="contact_section layout_padding">
    <div class="container px-0">
      <div class="heading_container ">
        <h2 class="">
          Contact Us
        </h2>
      </div>
    </div>
    <div class="container container-bg">
      <div class="row">
        <div class="col-lg-7 col-md-6 px-0">
          <!-- <div class="map_container">
            <div class="map-responsive">
              <iframe src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&q=Eiffel+Tower+Paris+France" width="600" height="300" frameborder="0" style="border:0; width: 100%; height:100%" allowfullscreen></iframe>
            </div>
          </div> -->
        </div>
        <div class="col-md-6 col-lg-5 px-0">
          <form action="/product/updateProduct" method="post" enctype="multipart/form-data">
            <div>
              <input type="number" id="id" name="id" placeholder="Id" value=<%= product.id %> />
            </div>
            <div>
              <input type="text" id="name" name="name" placeholder="Name" value=<%= product.name %> />
            </div>
            <div>
              <input type="number" id="price" name="price" placeholder="Price" value=<%= product.price %> />
            </div>
            <div>
              <input type="number" id="qty" name="qty" placeholder="Quantity" value=<%= product.qty %> />
            </div>
            <div>
              <input type="file" id="myfile" name="myfile" />
            </div>
            <div class="d-flex ">
              <button>
                Update Product
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

  <!-- end contact section -->

  <!-- info section -->

  <%- include('layouts/footer'); -%>

  <!-- end info section -->


  <%- include('layouts/scripts'); -%>

</body>

</html>